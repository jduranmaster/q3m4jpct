models/players/Chaos-Marine/Black-Legion_lower
{          
        {
                map models/players/Chaos-Marine/relfect-grey.tga
                tcGen environment
                rgbGen lightingdiffuse
        }
	{
		map $whiteimage 
		blendfunc blend
		rgbGen identity
		alphaGen lightingSpecular
	}
        {
                map models/players/Chaos-Marine/Black-Legion_lower.tga
                blendFunc blend
