models/players/Chaos-Marine/Nightlord_lower
{          
        {
                map models/players/Chaos-Marine/relfect-gold.tga
                tcGen environment
                rgbGen lightingdiffuse
        }
	{
		map $whiteimage 
		blendfunc blend
		rgbGen identity
		alphaGen lightingSpecular
	}
