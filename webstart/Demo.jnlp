<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+" codebase="http://q3m4jpct.googlecode.com/svn/webstart" href="Demo.jnlp">
        <application-desc main-class="q3m.demo.Demo" />
        <information>
                <title>q3m4jPCT Demo</title>
                <vendor>lotz.it</vendor>
                <homepage href="http://code.google.com/p/q3m4jpct" />
                <description>A simple demo showing a Quake III model.</description>
                <offline-allowed/>
        </information>
        <security>
                <all-permissions/>
        </security>
        <resources>
                <nativelib href="Demo.jar"/>
                <j2se version="1.5+" initial-heap-size="128m" max-heap-size="512m" />
        </resources>
        <resources os="Windows">
                <j2se version="1.5+"/>
                <property name="java.library.path" value="." />
        </resources>
        <resources os="Linux">
                <j2se version="1.5+"/>
                <property name="java.library.path" value="." />
        </resources>
        <resources os="Mac OS X">
                <j2se version="1.5+"/>
                <property name="java.library.path" value="." />
        </resources>
</jnlp>